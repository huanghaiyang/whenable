{
  "name": "whenable",
  "version": "0.1.0",
  "description": "case when then else",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run bundle && mocha",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huanghaiyang/whenable.git"
  },
  "keywords": [
    "when",
    "then",
    "case",
    "else",
    "elsewhen",
    "try",
    "catch"
  ],
  "author": "huanghaiyang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huanghaiyang/whenable/issues"
  },
  "homepage": "https://github.com/huanghaiyang/whenable#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "mocha": "^6.2.2",
    "rollup": "^1.27.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.7.2"
  }
}